set nocompatible
source $VIMRUNTIME/vimrc_example.vim
source $VIMRUNTIME/mswin.vim
behave mswin
set smartindent
set tabstop=4
set shiftwidth=4
set expandtab
set fileformat=unix
set fileencoding=utf8
set fileencodings=ucs-bom,utf8,prc
set nobackup
set nowritebackup
set noswapfile
set ignorecase              " Do case insensitive matching
"set incsearch               " Incremental search
"set hlsearch
set ttyfast
set ruler
set showcmd
set showmatch
